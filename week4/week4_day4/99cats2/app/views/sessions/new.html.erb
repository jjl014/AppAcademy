<h1>LOGIN</h1>
<%= render "shared/errors" %>
<form action="<%= session_url %>" method="post">
  <!-- post send it to # create -->
  <label for="user_name">Enter username</label>
  <input id="user_name" type="text" name="user[user_name]">
  <label for="password">Enter password</label>
  <input id="password" type="password" name="user[password]">
  <!-- this is passed to the user#create as params  -->
  <input type="submit" value="Login!">
</form>
